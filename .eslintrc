{
    "extends": ["react-app", "react-app/jest", "plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": [
            "warn",
            {
            "endOfLine": "auto",
            "singleQuote": true,
            "printWidth": 150
            }
        ],
        "import/first": 0
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "parser": "@babel/eslint-parser"
}